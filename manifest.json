{
  "name": "address-graphql",
  "vendor": "vtex",
  "version": "0.0.1",
  "builders": {
    "graphql": "1.x",
    "node": "6.x"
  },
  "dependencies": {},
  "settingsSchema": {
    "title": "Address GraphQL",
    "description": "Service related to address localization",
    "type": "object",
    "properties": {
      "apiKey": {
        "title": "API Key",
        "description": "API key used to access external geocoding service",
        "type": "string",
        "default": "API KEY"
      }
    }
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "maps.googleapis.com/maps",
        "path": "/api/geocode/json*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
